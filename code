#include <iostream>

using namespace std;

int main()
{
    int a, s = 0;
    
    cin >> a;
    
    if(a % 2 == 0)
    {
        s += a;
    }
    
    while(a != 0)
    {
        cin >> a;
        
        if(a % 2 == 0)
        {
            s += a;
        }
    }
    
    cout << s;
    
    return 0;
}
